      <Items xsi:type="CLUSTER" archetype_id="openEHR-EHR-CLUSTER.classification_glaucoma.v0" concept_name="Classification of glaucoma" path="/data[at0001]/items[at0043]">
        <Rule path="/items[at0002]">
          <constraint xsi:type="textConstraint">
            <termQueryId terminologyID="Snomed" terminologyLang="en-GB" queryName="Classification of glaucoma" />
          </constraint>
        </Rule>
