      <Rule path="/data[at0001]/items[at0002]">
        <constraint xsi:type="textConstraint">
          <termQueryId terminologyID="Snomed" terminologyLang="en-GB" queryName="Glaucoma treatments" />
        </constraint>
      </Rule>
      <Rule path="/data[at0001]/items[at0003]">
        <constraint xsi:type="textConstraint">
          <includedValues>Glaucoma stable</includedValues>
          <includedValues>Visual field loss</includedValues>
          <includedValues>Progressive excavation of the papilla</includedValues>
          <includedValues>IOP unstable</includedValues>
        </constraint>
      </Rule>
    </Item>
